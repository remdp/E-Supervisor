<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rates_graph"
    app:startDestination="@id/rates">

    <fragment
        android:id="@+id/rates"
        android:name="com.euromix.esupervisor.screens.main.tabs.rates.RatesFragment"
        android:label="@string/rates" >
        <argument
            android:name="state"
            app:argType="com.euromix.esupervisor.screens.main.tabs.rates.RatesViewModel$ViewState"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="screenTitle"
            app:argType="string"
            android:defaultValue="" />
        <action
            android:id="@+id/action_rates_to_manufacturers_rates"
            app:destination="@id/manufacturers_rates" />
    </fragment>
    <fragment
        android:id="@+id/manufacturers_rates"
        android:name="com.euromix.esupervisor.screens.main.tabs.rates.RatesFragment"
        android:label="@string/rates" >
        <argument
            android:name="state"
            app:argType="com.euromix.esupervisor.screens.main.tabs.rates.RatesViewModel$ViewState"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="screenTitle"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
</navigation>